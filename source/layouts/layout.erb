                    <ul class="usa-sidenav-sublist">
                      <li class="usa-sidenav-item">
                        <a href="envisioning.html"<%= is_page_selected("/envisioning.html") %>>Envisioning Design</a>
                      </li>
                      <li class="usa-sidenav-item">
                        <a href="ideation.html"<%= is_page_selected("/ideation.html") %>>Ideation</a>
                      </li>
                      <li class="usa-sidenav-item">
                        <a href="iteration.html"<%= is_page_selected("/iteration.html") %>>Iteration</a>
                      </li>
                      <li class="usa-sidenav-item">
                        <a href="feedback.html"<%= is_page_selected("/feedback.html") %>>Feedback</a>
                      </li>
                    </ul>
                  </li>

                  <li class="usa-sidenav-item">
                    <a href="productsservices.html"<%= is_page_selected("/productsservices.html") %>>Products & Services</a>
                  </li>
                  <li class="usa-sidenav-item">
                    <a href="orgmaturity.html"<%= is_page_selected("/orgmaturity.html") %>>Organizational Maturity</a>
                  </li>
                  <li class="usa-sidenav-item">
                    <a href="designimplementation.html"<%= is_page_selected("/designimplementation.html") %>>Design & Implementation</a>
                  </li>
                  <li class="usa-sidenav-item">
                    <a href="whatsnext.html"<%= is_page_selected("/whatsnext.html") %>>What's Next</a>
                  </li>
                  <li class="usa-sidenav-item">
                    <a href="glossary.html"<%= is_page_selected("/glossary.html") %>>Glossary</a>
                  </li>
                  <li class="usa-sidenav-item">
                    <a href="acknowledgements.html"<%= is_page_selected("/acknowledgements.html") %>>Acknowledgements</a>
                  </li>
                </nav>
              </aside>
              <div class="usa-layout-docs-main_content desktop:grid-col-9 usa-prose">
                <%= yield %>
              </div>
            </div>
          </div>
        </main>
        <footer class="usa-footer usa-footer-slim" role="contentinfo">
          <div class="grid-container usa-footer-return-to-top">
            <a href="#">Return to top</a>
          </div>
          <div class="usa-footer-primary-section">
            <div class="grid-row grid-row maxw-desktop margin-x-auto desktop:padding-x-4">
